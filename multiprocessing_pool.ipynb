      "[1, 4, 9, 16, 25]\n"
     ]
    }
   ],
   "source": [
    "array = [1,2,3,4,5]\n",
    "p = Pool()\n",
    "p.map(f, array)\n",
    "\n",
    "result=[]\n",
    "for n in array:\n",
    "    result.append(f(n))\n",
    "    \n",
    "print(result)"
   ]
