   "source": [
    "import time\n",
    "max_time_to_live=600\n",
    "\n",
    "mytime='_'.join( ['raf-auto'] + [ \n",
    "    str(x) for x in (\n",
    "        lambda x, y: (int(x)+max_time_to_live,str(int(x*y) % y / y).split('.')[-1])\n",
    "    ) (time.time(), 1e15) \n",
    "] )\n",
    "\n",
    "print(mytime)"
   ]
  },
